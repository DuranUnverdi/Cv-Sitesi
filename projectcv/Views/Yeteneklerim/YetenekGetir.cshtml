@model projectcv.Models.Entity.TBLSKILLS
@{
ViewBag.Title = "YetenekGetir";
Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("Guncelle", "Yeteneklerim", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.ID)
        @Html.TextBoxFor(m => m.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.SKILL)
        @Html.TextBoxFor(m => m.SKILL, new { @class = "form-control" })
        <br />
        <button class="btn btn-primary">Yetenek Güncelle</button>
    </div>
}

